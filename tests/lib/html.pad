from "lib/html.pad" import HtmlParser

def case1():
	puts("Test lib/list case1")

	p = HtmlParser()
	err = p.parse("<h1>title</h1>")
	assert(err == nil)
	assert(p.document)
	childs = p.document.childs
	assert(len(childs) == 1)
	assert(childs[0].type == "normal")
	assert(childs[0].name == "h1")
	assert(childs[0].childs[0].type == "text")
	assert(childs[0].childs[0].content == "title")

	err = p.parse("<h1 id=\"main\" class=\"gray\">title</h1>")
	assert(err == nil)
	assert(p.document)
	childs = p.document.childs
	assert(len(childs) == 1)
	assert(len(childs[0].attrs) == 2)
	assert(childs[0].attrs["id"] == "main")
	assert(childs[0].attrs["class"] == "gray")
	assert(childs[0].childs[0].content == "title")
end

def allTest():
	puts("Test lib/list all")

	case1()
end

def main(name):
	puts("Test lib/list")

	if name == "all":
		allTest()
	elif name == "case1":
		case1()
	end

	puts("Done lib/list")
end
@}
