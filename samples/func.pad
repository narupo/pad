{@
    def show(name):
        puts("Title:", name)
        block footer: end
        return name, nil
    end

    def dump(name) extends show:
        inject footer:
            def inner():
                puts("Welcome to my", name)
            end
            inner()
        end
        return super(name)
    end

    result, err = dump("Homepage")
    puts(result, err)
@}